		><%= f.label :date_of_birth %>
  <%= f.date_select :date_of_birth, :start_year => 1900 %></div>
  
  <div class="todo-popover" 
		data-original-title="FIRSTNA MMMMMmmmmmmmmmmmmmmmmmmm?" 
		data-content="We'll contact you via ygbhnkuvftgybh. Don't worry, we won't spam your mailboxes."
		
		><%= f.label :ethnicity %>
  <%= f.text_field :ethnicity %></div>
  
  <div class="todo-popover" 
		data-original-title="FIRSTNA MMMMMmmmmmmmmmmmmmmmmmmm?" 
		data-content="We'll contact you via ygbhnkuvftgybh. Don't worry, we won't spam your mailboxes."
		
		><%= f.label :city %>
  <%= f.text_field :city %></div>
  
  <div class="todo-popover" 
		data-original-title="FIRSTNA MMMMMmmmmmmmmmmmmmmmmmmm?" 
		data-content="We'll contact you via ygbhnkuvftgybh. Don't worry, we won't spam your mailboxes."
		
		><%= f.label :state %>
  <%= f.text_field :state %></div>

	<div class="todo-popover" 
		data-original-title="FIRSTNA MMMMMmmmmmmmmmmmmmmmmmmm?" 
		data-content="We'll contact you via ygbhnkuvftgybh. Don't worry, we won't spam your mailboxes."
		
		><%= f.label :zip_code %>
  <%= f.text_field :zip_code %></div>
  
  <div><%= f.submit "Sign up" %></div>
<% end %>

